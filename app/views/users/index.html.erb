<%= render 'layerbegin' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">

    <div class="ibox-title">
      <h3>用户</h3>
    </div>


    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">帐号</th>
          <th class="text-center">密码</th>
          <th class="text-center">类型</th>
          <th class="text-center">次数/截止日期</th>
          <th class="text-center">权限</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
            <tr>
              <td><%= user.login%></td>
              <td></td>

              <td><%= user.logintype%></td>
              <td></td>
              <td><%= user.status%></td>
              <td>
                <%= link_to '', edit_user_path(user),class:'fa fa-edit' %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <%= link_to '', user,class:'fa fa-trash', method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>




        </tbody>
      </table>
    </div>
  </div>
</div>


  <tbody>



  </tbody>
</table>
<br>
<div class="text-center">

  <%= will_paginate @users, previous_label:'上一页', next_label: '下一页' %>

</div>
<%= link_to "新建用户", new_user_path('user'),class: "btn btn-w-m btn-success mylink pull-right" ,'data-no-turbolink'=>true%>


<br>

