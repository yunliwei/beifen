<p id="notice"><%= notice %></p>

<h1>Listing Progres</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @progres.each do |progre| %>
      <tr>
        <td><%= link_to 'Show', progre %></td>
        <td><%= link_to 'Edit', edit_progre_path(progre) %></td>
        <td><%= link_to 'Destroy', progre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Progre', new_progre_path %>
